sudo apt install xz-utils
tar -xf  /home/jorj/Desktop/popap/uc?id=1WsagOqZaGm7gWDt1Q54ol7ucLphy8XPA
sudo dpkg -i opera-stable_82.0.4227.23_amd64.deb 

